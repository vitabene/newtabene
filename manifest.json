{
  "name": "new tab",
  "description": "a simple and clean newtab extension for Google Chrome",
  "version": "2.0",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "assets/favicon.png"
  },
  "manifest_version": 2
}
